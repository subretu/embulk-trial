in:
  type: postgresql
  host: postgres
  user: postgres
  password: "postgres"
  database: postgres
  query: |
    select
      opstime
      ,value
    from
      sample_date_summary
    ;

out:
  type: postgresql
  mode: truncate_insert
  default_timezone: "Asia/Tokyo"
  host: postgres
  port: 5432
  user: postgres
  password: "postgres"
  database: postgres
  table: output_sample